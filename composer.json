{
    "name": "jolicode/automapper-bundle",
    "type": "symfony-bundle",
    "description": "JoliCode AutoMapper Symfony Bundle",
    "keywords": ["bundle", "symfony"],
    "homepage": "https://github.com/jolicode/automapper-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Joel Wurtz",
            "email": "jwurtz@jolicode.com"
        },
        {
            "name": "Baptiste Leduc",
            "email": "baptiste.leduc@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "jolicode/automapper": "^8.0"
    },
    "require-dev": {
        "moneyphp/money": "^4.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpunit/phpunit": "^8.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/uid": "^6.3",
        "symfony/yaml": "^6.3"
    },
    "conflict": {
        "symfony/framework-bundle": "5.1.0"
    },
    "autoload": {
        "psr-4": {
            "AutoMapper\\Bundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AutoMapper\\Bundle\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/Resources/App/AppKernel.php"
        ]
    },
    "config": {
        "process-timeout": 1800,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
